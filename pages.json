[
    { "uri": "/", "local": "/index" },
    { "uri": "/head", "local": "/head" },
    { "uri": "/foot", "local": "/foot" },
    { "uri": "/projects", "local": "/projects", "content_type": "use_request" },
    { 
	"uri": "/scripts", 
	"type": "collection", 
	"content_type": "javascript",
	"collection": [
	    "/scripts/jquery-1.7.2.min.js",
	    "/scripts/json2.js",
	    "/scripts/underscore-min.js",
	    "/scripts/backbone-min.js",
	    "/scripts/controller.js",
	    "/scripts/views/*.js",
	    "/scripts/collections/models/*.js",
	    "/scripts/collections/*.js",
	    "/scripts/templates/*.jst"
	]
    },
    {
	"uri": "/style",
	"type": "collection",
	"content_type": "css",
	"collection": [
	    "/style/*.css"
	]
    }
]
